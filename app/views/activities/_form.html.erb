<%=form_with model: @activity do |f| %>

  <%=f.label :category_id%>
  <%=f.collection_radio_buttons :category_id, Category.all, :id, :name%>
  <br>
  <br>

  <%=f.label :name%>
  <%=f.text_field :name%>
  <br><br>

  <%=f.label :address%>
  <%=f.text_field :address%>
  <br><br>

  <%=f.label :city%>
  <%=f.text_field :city%>
  <br><br>

  <%=f.label :state%>
  <%=f.text_field :state%>
  <br><br>

  <%=f.label :zipcode%>
  <%=f.text_field :zipcode%>
  <br><br>

  <%= f.label :description%>
  <%= f.text_area :description%>
  <br>

  <%= f.hidden_field :user_id, :value=> session[:user_id] %><br>


  <%=f.submit%>
<%end%>
