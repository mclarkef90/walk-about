<h1>Categories</h1>
<ul>
<% @categories.each do |category| %>
  <li>
    <%= link_to category.name, category_path(category) %><br>
    <ol>
      <% category.activities.each do |activity| %>
        <li><%= link_to activity.name, activity_path(activity)%></li>
      <%end%>
      <br>
    </ol>
  </li>
<%end%>
</ul>
